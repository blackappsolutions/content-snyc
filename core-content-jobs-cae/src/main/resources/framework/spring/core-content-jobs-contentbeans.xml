<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <description>
    Generated content bean mappings.
  </description>

  <bean name="abstractContentBean" scope="prototype" abstract="true">
    <property name="settingsService" ref="settingsService"/>
  </bean>

  <bean name="contentBeanFactory:ContentJob" class="de.bas.content.beans.ContentJobImpl" scope="prototype" parent="abstractContentBean">
    <description>contentbean mapping for contents of type 'ContentJob'</description>
  </bean>

</beans>
